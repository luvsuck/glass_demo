<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.luvsic.demo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SET_WALLPAPER" />

    <uses-feature android:name="android.permission.camera" />

    <application
        android:name="com.luvsic.demo.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.luvsic.demo.fragment.ContainerActivity"
            android:exported="true" />
        <activity
            android:name="com.luvsic.demo.PointDetailsActivity"
            android:exported="true" />
        <activity
            android:name="com.luvsic.demo.ServiceItemDetailsActivity"
            android:exported="true" />
        <activity
            android:name="com.luvsic.demo.ServiceItemActivity"
            android:exported="true" />
        <activity
            android:name="com.luvsic.demo.UploadActivity"
            android:exported="true" />
        <activity
            android:name="com.luvsic.demo.ImageListActivity"
            android:exported="true" />
        <activity
            android:name="com.luvsic.demo.AttachmentManagementActivity"
            android:exported="true" />
        <activity
            android:name="com.luvsic.demo.AfterSaleSummaryActivity"
            android:exported="true" />
        <activity
            android:name="com.luvsic.demo.TransactionRecordActivity"
            android:exported="true" />
        <activity
            android:name="com.luvsic.demo.SoundRecordActivity"
            android:exported="true" />
        <activity
            android:name="com.luvsic.demo.CameraActivity"
            android:exported="true" />
        <activity
            android:name="com.luvsic.demo.InstructActivity"
            android:exported="true" />
        <activity
            android:name="com.luvsic.demo.TaskHeaderActivity"
            android:exported="true"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.luvsic.demo.FaultDescriptionActivity"
            android:exported="true"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.luvsic.demo.IMUActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.luvsic.demo.MainActivity"
            android:exported="true"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="design_width_in_dp"
            android:value="640" />
        <meta-data
            android:name="design_height_in_dp"
            android:value="360" />
    </application>

</manifest>